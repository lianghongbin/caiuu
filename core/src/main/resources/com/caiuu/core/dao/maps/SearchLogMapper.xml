<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.caiuu.core.dao.SearchLogMapper" >
    <resultMap id="BaseResultMap" type="com.caiuu.core.entity.SearchLog" >
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="key" property="key" jdbcType="VARCHAR" />
        <result column="result" property="result" jdbcType="INTEGER" />
        <result column="ip" property="ip" jdbcType="VARCHAR" />
        <result column="hit" property="hit" jdbcType="INTEGER" />
        <result column="createtime" property="createTime" jdbcType="TIMESTAMP" />
    </resultMap>

    <select id="selectAll" parameterType="SearchLog">
        select from searchlog order by id desc
    </select>

    <select id="selectTop" resultType="SearchLog">
        select * from searchtop order by hit desc
    </select>
</mapper>